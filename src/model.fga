model
    schema 1.2

type user

type tenant
  relations
    define member: [user]
    define admin: member

type site
  relations
    define parent: [tenant]
    define member: [user] or member from parent
    define admin: [user] or admin from parent

type vehicle
    relations
        define driver: [user]
        define owner: [user]
